-- Excercise 1
SELECT 
sum(case when device_type in ('tablet','phone') then 1 else 0 
end) as mobile_views,
sum(case when device_type = 'laptop' then 1 else 0
end) as laptop_views
from viewership;

-- Excercise 2
select *,
case when ((x+y)>z) and ((x+z)>y) and ((y+z)>x)
then 'Yes' else 'No'
end as 'triangle'
 from triangle;

